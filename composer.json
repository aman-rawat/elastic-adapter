{
    "name": "aman-rawat/elastic-adapter",
    "description": "Adapter for official PHP Elasticsearch client",
    "version": "1.0.1",
    "keywords": [
        "elastic",
        "elasticsearch",
        "adapter",
        "client",
        "php"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aman Rawat",
            "email": "amanrwt99@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ElasticAdapter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElasticAdapter\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "elasticsearch/elasticsearch": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^0.12.32",
        "orchestra/testbench": "^6.20"
    },
    "config": {
        "bin-dir": "bin"
    }
}
